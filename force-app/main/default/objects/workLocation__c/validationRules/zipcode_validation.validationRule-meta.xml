<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>zipcode_validation</fullName>
    <active>true</active>
    <errorConditionFormula>ZipCode__c &lt; 10000 ||  ZipCode__c &gt;99999</errorConditionFormula>
    <errorMessage>Zipcode must be in 5 digit format</errorMessage>
</ValidationRule>
